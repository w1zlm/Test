Create = true
for i, v in pairs(game.Players.LocalPlayer:GetChildren()) do
	if v.Name == "Newest Gui Version" then
		v.Value = "1.0"
		Create = false
	end
end
if Create == true then
	local Version = Instance.new("StringValue")
	Version.Parent = game:GetService("Players").LocalPlayer
	Version.Name = "Newest Gui Version"
	Version.Value = "1.0"
end
